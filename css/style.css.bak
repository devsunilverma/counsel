*{box-sizing:border-box;margin:0px;padding:0px;}
/*header*/
header{background:url("../image/main.jpg") no-repeat center center;background-size:cover;width:100%;height:100vh;padding-bottom:13.063rem;}
header .container,section .container,footer .container{max-width:1170px;}
.navbar{padding-top:3.188rem;}
.navbar-brand{font:2.813rem/0.2rem 'Montserrat', sans-serif;font-weight:700;color:#FDE428;}
.navbar-brand:hover{color:#FDE428;}
.navbar-brand span{font:0.75rem/3.188rem 'Montserrat', sans-serif;font-weight:700;letter-spacing:4px;color:#ffffff;}
.navbar ul li a{font:0.875rem/3rem 'Montserrat', sans-serif;color:#ffffff;box-shadow:inset 0 0 0 0 #ffffff;transition:color .3s ease-in-out, box-shadow .3s ease-in-out;}
.navbar ul li a:hover, .navbar ul li a:focus{color:#001C37;box-shadow: inset 200px 0 0 0 #FDE428;border-radius:5px;}
.slideerimg{padding-top:14.613rem;}

.card-title{font:3.188rem/3.188rem 'Montserrat', sans-serif;font-weight:700;text-align:end;color:#ffffff;top:4.813rem;right:13.625rem;}
.card-text{font:1.313rem/3.188rem 'Montserrat', sans-serif;font-weight:700;color:#FDE428;top:16rem;right:14rem;}
/*pre btn*/
.carousel-control-prev{left:1%;top:64%;background-image:none;border:1px solid #FDE428;height:13px;width:11px;}
.carousel-control-prev-icon,.carousel-control-next-icon{background-image:none;}
.carousel-control-next{right:80%;top:64%;background-image:none;border:1px solid #FDE428;height:13px;width:11px;}
.carousel-control-prev{color:#FDE428;font:0.775rem/3.588rem 'Myriad', sans-serif;}
.carousel-control-next{color:#FDE428;font:0.775rem/3.588rem 'Myriad', sans-serif;}
.icontextpre{padding-left:117px;}
.icontextnxt{padding-right:87px;}



/*section-1*/
.sec-1text{padding:5.125rem 0;}
.sec-1text h2{font:1.938rem/1.938rem 'Montserrat', sans-serif;font-weight:700;color:#002E5B;}
.sec-1text h4{font:1.313rem/1.563rem  'Noto Sans', sans-serif;color:#666666;padding-top:1.75rem;}
.sec-1text p{font:1rem/1.563rem 'Noto Sans', sans-serif;color:#333333;padding-top:1.5rem;}
.sec-1text h5{font:1rem/1.563rem 'Montserrat', sans-serif;font-weight:700;color:#002E5B;padding-top:2.1rem;}
.gray-rect{background:#A0A0A0;position:relative;z-index:2;}
.gray-rect:before{content:"";width:60%;height:524px;background:#FDE327;position:absolute;z-index:1;top:-100px;left:96px;}

/*section-2*/
.section-2{background:#F4F4F4;padding-top:6.313rem;padding-bottom:6.125rem;}
.sec-2text h2{font:1.938rem/1.938rem 'Montserrat', sans-serif;font-weight:700;color:#002E5B;}
.sec-2text h4{font:1.313rem/1.563rem  'Noto Sans', sans-serif;color:#666666;padding-top:1.75rem;}
.sec-2text p{font:1rem/1.563rem 'Noto Sans', sans-serif;color:#333333;padding-top:1.5rem;}
.sec-2text h5{font:1rem/1.563rem 'Montserrat', sans-serif;font-weight:700;color:#002E5B;padding-top:2.1rem;}

/*section-3*/
.section-3{padding-bottom:6rem;}
.contact{background-color:#002E5B;padding:1.5rem 0;}
.contact-text p{font:1.313rem/3.188rem 'Montserrat', sans-serif;}
.btn-warning{width:161px;height:44px;font:1rem/1.188rem 'Montserrat', sans-serif;font-weight:700;color:#002E5B;}
.sec-3-text{padding-top:6.125rem;}
.sec-3-text h2{font:1.938rem/1.938rem 'Montserrat', sans-serif;font-weight:700;color:#002E5B;}
.sec-3-text p{padding-top:1.625rem;font:1.313rem/2.625rem 'Noto Sans', sans-serif;color:#666666;}
.box-1{background-color:#69D1E7;}
.box-2{background-color:#ACD477;}
.box-3{background-color:#F49F76;}
.box-1,.box-2,.box-3{margin-top:2.438rem;}
.box-1text h3,.box-2text h3,.box-3text h3{padding-top:2.3rem; font:1.313rem/3rem 'Montserrat', sans-serif;font-weight:700;color:#002E5B;}
.box-1text p,.box-2text p,.box-3text p{padding-top:1.844rem;font:1rem/1.563rem 'Noto Sans', sans-serif;color:#666666;}

/*section-4*/
.section-4{background-color:#002E5B;padding:8.875rem 0;}
.counter, .numbertext h3{font:3rem/3.75rem 'Montserrat', sans-serif;}
.numbertext h4{font:0.875rem/3rem 'Noto Sans', sans-serif;}

/*sec-5*/
.yellow-rect{background:#FDE327;}
.blue-rect{background-color:#002E5B;top:6.25rem;right:0;}
.play-btn{width:50px;top:45%;left:45%;position:relative;transition:0.5s;border:5px solid #33577C;}

.sec-5text{padding-top:6.188rem;}
.sec-5text h2{font:1.938rem/1.938rem 'Montserrat', sans-serif;font-weight:700;color:#002E5B;}
.sec-5text h4{font:1.313rem/1.563rem  'Noto Sans', sans-serif;color:#666666;padding-top:1.75rem;}
.sec-5text p{font:1rem/1.563rem 'Noto Sans', sans-serif;color:#333333;padding-top:1.5rem;}
.sec-5text h5{font:1rem/1.563rem 'Montserrat', sans-serif;font-weight:700;color:#002E5B;padding:top:2.1rem;}
/*sec-6*/
.section-6{background-color:#002E5B;padding-top:7.813rem;padding-bottom:8.125rem;}
.sec-6text p{font:1.25rem/2.375rem 'Myriad', sans-serif;}
.sec-6text h6{font:0.875rem/3rem 'Montserrat', sans-serif;font-weight:700;}
/*section-7*/
.section-7{padding:6.313rem 0;}
.sec-7text h2{font:2rem/2rem 'Montserrat', sans-serif;font-weight:700;color:#002E5B;}
.sec-7text p{font:1.313rem/1.563rem  'Noto Sans', sans-serif;color:#666666;padding-top:1.563rem;}
.maptext{padding:2.6rem 0 2rem 2.438rem;}
.maptext h3{font:1rem/0.863rem 'Montserrat', sans-serif;font-weight:700;color:#002E5B;}
.maptext h6{font:1rem/1.313rem 'Noto Sans', sans-serif;color:#002E5B;}

/*footer*/
footer{background-color:#002E5B;padding:2rem 0;}
footer h5{font:1rem/2.813rem 'Montserrat', sans-serif;}
footer h5 span{color:#FFD400;}

@media only screen and (max-width:1199.98px) {
	/*header*/
	header .container,section .container,footer .container{max-width:905px;}
	/*section-1*/
	.sec-1text{padding:3.563rem 0;}
	.sec-1text h2{font:1.5rem/1.938rem 'Montserrat', sans-serif;}
	.sec-1text h4 {font: 1.1rem/1.563rem 'Noto Sans', sans-serif;}
	.gray-rect{height:290px;}
	.gray-rect:before{left:65px;}
	/*section-2*/
	.section-2{padding-bottom: 2.125rem;}
	.sec-2text h2{font:1.5rem/1.938rem 'Montserrat', sans-serif;}
	.sec-2text h4{padding-top:0.75rem}
	/*section-3*/
	.contact-text p{font: 1rem/3.188rem 'Montserrat', sans-serif;}
	.box-1text h3, .box-2text h3, .box-3text h3 {font: 1.1rem/3rem 'Montserrat', sans-serif;}
	
	/*section-4*/
	.section-4 {padding:4.875rem 0;}
	.sec-5text {padding-top: 4.188rem;}
	..sec-5text h2 {font: 1.8rem/1.938rem 'Montserrat', sans-serif;}
	.sec-5text h4{padding-top:0.75rem;}
	.blue-rect{height:249px;top:7.25rem;}
	
	/*section-6*/
	.section-6 {padding-top:3.813rem;padding-bottom:3.125rem;}
	.sec-6text p{font:1.1rem/2.375rem 'Myriad', sans-serif;}

@media only screen and (max-width:991.98px) {
	/*header*/
	header{height:auto;}
	header .container,section .container,footer .container{max-width:700px;}
	.navbar-brand{font:2.2rem/0.2rem 'Montserrat', sans-serif;}
	.navbar-brand span{font: 0.5rem/3.188rem 'Montserrat', sans-serif;}
	.navbar-nav li:last-of-type{display:none;}
	.slideerimg{padding-top:6.613rem;}
	.card-title{font:2.188rem/3.188rem 'Montserrat', sans-serif;}
}
@media only screen and (max-width: 767.98px) {}
@media only screen and (max-width: 575.98px) {

.tglnavbar{top:120px;}
.navbar-nav li:last-of-type{display:none;}


}
